msgid "Message"
msgstr "Сообщение"

msgctxt "the_app"
msgid "message"
msgstr "Сообщения"

msgid "Recipients"
msgstr "Адресаты"

msgid "Additional Recipients"
msgstr "Дополнительные адресаты"

msgid "Undefined recipient."
msgstr "Неопределенный адресат"

msgid "Messages"
msgstr "Сообщения"

msgid "Archived Messages"
msgstr "Архивированные сообщения"

msgid "Me"
msgstr "Я"

msgid "No messages"
msgstr "Сообщений нет"

msgid "Delete"
msgstr "Удалить"

msgid "Archive"
msgstr "В архив"

msgid "Undelete"
msgstr "Восстановить"

msgid "Please enter message..."
msgstr "Пожалуйста, введите сообщение..."

msgid "Send"
msgstr "Отправить "

msgid "Dear user,"
msgstr "Уважаемый пользователь,"

#, python-format
msgid "On %(date)s, you asked to send a message to the user '%(recipient)s'."
msgstr "%(date)s, Вы просили отправить сообщение пользователю '%(recipient)s'."

msgid "Your message has been rejected by the moderator"
msgstr "Ваше сообщение отклонено модератором"

msgid ", for the following reason:"
msgstr ", по следующей причине:"

#, python-format
msgid "'%(sender_first)s %(sender_last)s' has replied to your message from %(date)s:"
msgstr "’%(sender_first)s %(sender_last)s' ответил(а) на Ваше сообщение от %(date)s:"

#, python-format
msgid "You have received a copy of a response from the user '%(sender_first)s %(sender_last)s'."
msgstr "Вы получили копию ответа от пользователя '%(sender_first)s %(sender_last)s'"

#, python-format
msgid "You have received a message from the user '%(sender_first)s %(sender_last)s'."
msgstr "Вы получили сообщение от пользователя '%(sender_first)s %(sender_last)s'"

msgid "Follow this link to view the message on the site"
msgstr "Перейдите по этой ссылке для просмотра сообщения на сайте"

msgid ""
"Note: This message is issued by an automated system.\n"
"Do not reply, this would not be taken into account."
msgstr ""
"Примечание: Это автоматическое сообщение.\n"
"Не отвечайте на него, Ваш ответ не будет прочитан"

#, python-format
msgid "Message \"%(subject)s\" on the site %(sitename)s"
msgstr "Сообщение \"%(subject)s\" на сайте %(sitename)s"

msgid "Dear visitor,"
msgstr "Уважаемый посетитель,"

msgid "As a reminder, please find below the content of your message."
msgstr "В качестве напоминания ниже Вы найдете содержание Вашего сообщения."

#, python-format
msgid "On %(date)s, you sent a message to the user '%(sender_first)s %(sender_last)s'."
msgstr "%(date)s Вы отправили сообщение пользователю '%(sender_first)s %(sender_last)s‘."

msgid "Please find below the answer from your correspondent."
msgstr "Ниже Вы найдете ответ Вашего собеседника."

msgid "Thank you again for your interest in our services."
msgstr "Еще раз благодарим за интерес, проявленный к нашей работе."

msgid "For more comfort, we encourage you to open an account on the site."
msgstr "Чтобы Вам было еще удобнее, рекомендуем открыть аккаунт на этой странице."

msgid "The site administrator"
msgstr "Администратор сайта"

msgid "Inbox"
msgstr "Входящие"

msgid "Sent"
msgstr "Отправленные"

msgid "Trash"
msgstr "Корзина"

msgid "Write"
msgstr "Написать"

msgid "Reply"
msgstr "Ответить"

msgid "Recipient"
msgstr "Адресат"

msgid "Conversation"
msgstr "Беседа"

msgid "Answer"
msgstr "Ответ"

msgid "Reply..."
msgstr "Ответить..."

msgid "Select at least one object."
msgstr "Выберите хотя бы один объект"

msgid "Messages or conversations successfully archived."
msgstr "Сообщения или диалоги архивированы"

msgid "Messages or conversations successfully deleted."
msgstr "Сообщения или диалоги удалены"

msgid "Messages or conversations successfully recovered."
msgstr "Сообщения или диалоги восстановлены"
